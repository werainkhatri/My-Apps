<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textOut"
        android:layout_width="368dp"
        android:layout_height="70dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:text="=0"
        android:textAlignment="viewEnd"
        android:textSize="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/button"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:onClick="six"
        android:text="6"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/button16"
        app:layout_constraintTop_toBottomOf="@+id/button5" />

    <Button
        android:id="@+id/button15"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:layout_marginTop="4dp"
        android:onClick="sqroot"
        android:text="âˆšx"
        android:textAllCaps="false"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/button11"
        app:layout_constraintTop_toBottomOf="@+id/textOut" />

    <Button
        android:id="@+id/button16"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:layout_marginEnd="7dp"
        android:onClick="sub"
        android:text="-"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button4" />

    <Button
        android:id="@+id/button17"
        android:layout_width="183dp"
        android:layout_height="65dp"
        android:layout_marginEnd="7dp"
        android:onClick="equal"
        android:text="="
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button21" />

    <Button
        android:id="@+id/button19"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:onClick="decimal"
        android:text="."
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/button17"
        app:layout_constraintStart_toEndOf="@+id/button20"
        app:layout_constraintTop_toBottomOf="@+id/button23" />

    <Button
        android:id="@+id/button20"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="1dp"
        android:onClick="zero"
        android:text="0"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/button19"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button24" />

    <Button
        android:id="@+id/button21"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:layout_marginEnd="7dp"
        android:onClick="add"
        android:text="+"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button16" />

    <Button
        android:id="@+id/button22"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:onClick="three"
        android:text="3"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/button21"
        app:layout_constraintTop_toBottomOf="@+id/button" />

    <Button
        android:id="@+id/button23"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:onClick="two"
        android:text="2"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/button22"
        app:layout_constraintStart_toEndOf="@+id/button24"
        app:layout_constraintTop_toBottomOf="@+id/button2" />

    <Button
        android:id="@+id/button24"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:layout_marginStart="5dp"
        android:onClick="one"
        android:text="1"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/button23"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3" />

    <Button
        android:id="@+id/button2"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:onClick="five"
        android:text="5"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/button"
        app:layout_constraintStart_toEndOf="@+id/button3"
        app:layout_constraintTop_toBottomOf="@+id/button6" />

    <Button
        android:id="@+id/button3"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:layout_marginStart="5dp"
        android:onClick="four"
        android:text="4"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/button2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button7" />

    <Button
        android:id="@+id/button4"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:layout_marginEnd="7dp"
        android:onClick="multiply"
        android:text="*"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button8" />

    <Button
        android:id="@+id/button5"
        android:layout_width="92dp"
        android:layout_height="65dp"
        android:onClick="nine"
        android:text="9"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/button6"
        app:layout_constraintTop_toBottomOf="@+id/button14" />

    <Button
        android:id="@+id/button6"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:layout_marginTop="2dp"
        android:onClick="eight"
        android:text="8"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/button5"
        app:layout_constraintStart_toEndOf="@+id/button7"
        app:layout_constraintTop_toBottomOf="@+id/button15" />

    <Button
        android:id="@+id/button7"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:layout_marginStart="5dp"
        android:onClick="seven"
        android:text="7"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/button6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button11" />

    <Button
        android:id="@+id/button8"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="7dp"
        android:onClick="divide"
        android:text="/"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textOut" />

    <Button
        android:id="@+id/button11"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="5dp"
        android:onClick="clearAll"
        android:text="AC"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textOut" />

    <Button
        android:id="@+id/button14"
        android:layout_width="93dp"
        android:layout_height="65dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="1dp"
        android:onClick="power"
        android:text="x^y"
        android:textAllCaps="false"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/button8"
        app:layout_constraintTop_toBottomOf="@+id/textOut" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="368dp"
        android:layout_height="70dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:textAlignment="viewEnd"
        android:textSize="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>